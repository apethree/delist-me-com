{
  "name": "delistme",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:marketing": "turbo run dev --filter=marketing",
    "dev:dashboard": "turbo run dev --filter=dashboard",
    "dev:mobile": "turbo run dev --filter=delistme-mobile",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "supabase": "supabase",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "optionalDependencies": {
    "supabase": "^2.72.9"
  }
}
